@model Veiculo

@{
    ViewData["Title"] = "Cadastro de Veículo";    
}

@section Links {
    <a asp-action="index" asp-controller="veiculo">Veículos</a>
}

<h1>Cadastro de Veículo</h1>

<mensagem texto="@TempData["msg"]"></mensagem> <!--Criar a tag para mensagem: -->

<form asp-action="cadastrar">
   
    <!-- Incluir o conteúdo da página _Form.cshtml -->
    <partial name="_Form"/>
    
    <botao texto="Salvar" class="btn btn-outline-success"></botao>
    
    <a asp-action="index" class="btn btn-outline-dark">Veículos</a>
</form>

